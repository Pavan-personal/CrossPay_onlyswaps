{
    "version": 2,
    "buildCommand": "npm install && npx prisma generate",
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "env": {
        "FRONTEND_URL": "https://cross-pay-onlyswaps.vercel.app"
    }
}